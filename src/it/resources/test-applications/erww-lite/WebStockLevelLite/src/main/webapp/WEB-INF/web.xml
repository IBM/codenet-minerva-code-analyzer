		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<persistence-unit-ref>
		<persistence-unit-ref-name>StockQuery/entitymanagerfactory</persistence-unit-ref-name>
		<persistence-unit-name>StockQuery</persistence-unit-name>
	</persistence-unit-ref>
	<resource-ref>
		<description>
		</description>
		<res-ref-name>jdbc/ERWWDataSourceWithJaasAlias</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
