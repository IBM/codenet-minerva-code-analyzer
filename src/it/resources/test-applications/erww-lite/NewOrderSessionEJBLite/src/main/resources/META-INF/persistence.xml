<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="NewOrderSessionEJBLite">
        <jta-data-source>jdbc/ERWWDataSourceWithPwd</jta-data-source>
        <class>warehousejpa.WarehouseJPA</class>
		<class>districtjpa.DistrictJPA</class>
		<class>customerjpa.CustomerJPA</class>
		<class>stockjpa.StockJPA</class>
		<class>orderjpa.OrderJPA</class>
		<class>orderlinejpa.OrderlineJPA</class>
		<class>neworderjpa.NewOrderJPA</class>
		<class>itemjpa.ItemJPA</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
    </persistence-unit>
</persistence>
